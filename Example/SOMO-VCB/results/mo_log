***** Training Set ***** 
trainingSet.size    = 24000 [Size of the complete training set] 
trainingSet.dim     = 2 [Dimension of the training pattern-vectors] 

***** RBF Network ***** 
neuralModel.nn      = 5 [Number of neurons of the RBF network] 
neuralModel.muRange = [-5.000000 5.000000] [Range for the mu parameter] 
neuralModel.sRange  = [0.100000 2.000000] [Range for the sigma parameter (st.dev.)] 
neuralModel.wRange  = [-20.000000 20.000000] [Range for the network weights] 

***** VCB Algorithm ***** 
paramVCB.nbat       = 10 [Number of mini-batches per VCB cycle] 
paramVCB.npat       = 240 [Number of patterns used per mini-batch set] 
paramVCB.cycles     = 10 [Number of cycles of the VCB algorithm] 
paramVCB.NNeval     = 1000000 [Number of single-pattern evaluations of the RBF network] 

***** MOPSO and Problem ***** 
paramMO.nobj        = 2 [Number of objective functions] 
paramMO.nexp        = 2 [Number of experiments] 
paramMO.dim         = 21 [Dimension of the optimization problem] 
paramMO.ss          = 10 [Swarm size] 
paramMO.rs          = 10 [Repository size] 
paramMO.maxit       = 100 [Maximum MOPSO iterations] 
paramMO.repNoImp    = 20 [Maximum iterations with no repository change] 
paramMO.W           = 0.729 [MOPSO inertia weight] 
paramMO.C1          =  1.49 [MOPSO cognitive parameter] 
paramMO.C2          =  1.49 [MOPSO social parameter] 
paramMO.grid        = 20 [Number of grid points per dimension] 
paramMO.maxv        = 0.200000 [Maximum velocity as percentage of the search range] 
paramMO.mutp        = 0.250000 [Percentage of uniform mutation] 
evalStrategy        = 0 [Pareto set evaluation strategy] 
